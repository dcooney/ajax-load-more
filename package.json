{
	"name": "ajax-load-more",
	"version": "6.0.0",
	"description": "Ajax Load More WordPress Plugin",
	"main": "core/src/js/ajax-load-more.js",
	"scripts": {
		"dev": "npm run start",
		"start": "webpack -w --mode development --config=webpack/dev.config.js",
		"build": "webpack -p --mode production --config=webpack/prod.config.js && npm run clean && mkdirp ./_out && npm run pot && npm run build:files && npm run build:dirs",
		"pot": "composer run pot",
		"clean": "cd ./core/dist/ && del-cli js/ajax-load-more.js.map",
		"build:files": "cpy 'ajax-load-more.php' 'README.txt' 'LICENSE.txt' 'uninstall.php' ./_out",
		"build:dirs": "cpy './admin' './core' './includes' './lang' ./_out --parents"
	},
	"author": "connekt",
	"private": true,
	"repository": {
		"private": true
	},
	"browserslist": [
		"last 2 versions",
		"ie >= 9",
		"and_chr >= 2.3"
	],
	"dependencies": {
		"@babel/polyfill": "^7.10.1",
		"crypto-js": "^4.1.1",
		"es6-promise": "^4.1.1",
		"focus-options-polyfill": "^1.4.0",
		"lodash": "^4.17.15"
	},
	"devDependencies": {
		"autoprefixer": "^7.1.4",
		"axios": "^0.24.0",
		"babel-core": "^6.25.0",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^7.1.1",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-regenerator-runtime": "^6.5.0",
		"copy-webpack-plugin": "^6.4.1",
		"css-loader": "^0.28.7",
		"del-cli": "^3.0.1",
		"eslint": "^5.15.1",
		"eslint-loader": "^2.1.2",
		"extract-text-webpack-plugin": "4.0.0-alpha.0",
		"file-loader": "^2.0.0",
		"gulp": "3.9.1",
		"gulp-autoprefixer": "latest",
		"gulp-babel": "^7.0.1",
		"gulp-clean-css": "latest",
		"gulp-concat": "latest",
		"gulp-jshint": "latest",
		"gulp-notify": "latest",
		"gulp-rename": "latest",
		"gulp-sass": "latest",
		"gulp-sequence": "latest",
		"gulp-uglify": "latest",
		"gulp-util": "latest",
		"gulp-watch": "latest",
		"imagesloaded": "^4.1.4",
		"jshint": "latest",
		"node-sass": "^9.0.0",
		"postcss-import": "^10.0.0",
		"postcss-loader": "^2.0.6",
		"qs": "^6.6.0",
		"sass": "^1.54.5",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.18.2",
		"url-loader": "^1.1.2",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	}
}
